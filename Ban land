local Players = game:GetService("Players")
local player = Players.LocalPlayer

-- ‡∏£‡∏≤‡∏¢‡∏ä‡∏∑‡πà‡∏≠ UserId ‡∏ó‡∏µ‡πà‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£‡πÅ‡∏ö‡∏ô
local bannedUsers = { 7889642399, 7948347827, 6134412567, 1790315511, 6003862605, 2652453850, 0, 0, }

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏õ‡∏¥‡∏î Anti-Kick
local function disableAntiKick()
    local mt = getrawmetatable(game)
    local oldNamecall = mt.__namecall
    local oldIndex = mt.__index
    setreadonly(mt, false)

    -- ‡∏õ‡∏¥‡∏î Hook __index
    mt.__index = newcclosure(function(self, method)
        if self == player and method:lower() == "kick" then
            return rawget(self, method)
        end
        return oldIndex(self, method)
    end)

    -- ‡∏õ‡∏¥‡∏î Hook __namecall
    mt.__namecall = newcclosure(function(self, ...)
        local method = getnamecallmethod()
        if self == player and method:lower() == "kick" then
            return oldNamecall(self, ...)
        end
        return oldNamecall(self, ...)
    end)

    setreadonly(mt, true)
end

-- ‡∏ñ‡πâ‡∏≤‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏≠‡∏¢‡∏π‡πà‡πÉ‡∏ô‡∏•‡∏¥‡∏™‡∏ï‡πå‡πÅ‡∏ö‡∏ô
if table.find(bannedUsers, player.UserId) then
    disableAntiKick() -- ‡∏õ‡∏¥‡∏î Anti-Kick ‡∏Å‡πà‡∏≠‡∏ô
    task.wait(0.2) -- ‡∏£‡∏≠‡πÉ‡∏´‡πâ‡∏Å‡∏≤‡∏£‡πÅ‡∏Å‡πâ‡πÑ‡∏Ç‡∏°‡∏µ‡∏ú‡∏•

    -- ‡πÄ‡∏ï‡∏∞‡∏≠‡∏≠‡∏Å‡∏ã‡πâ‡∏≥‡πÜ ‡∏à‡∏ô‡∏Å‡∏ß‡πà‡∏≤‡∏à‡∏∞‡∏≠‡∏≠‡∏Å
    while true do
        player:Kick("You got banned from the scriptü§£ü§°")
        task.wait(0.1) -- ‡∏•‡∏î‡∏Å‡∏≤‡∏£‡πÉ‡∏ä‡πâ CPU
    end

    -- **‡∏™‡πà‡∏á‡∏Ñ‡πà‡∏≤ true ‡∏Å‡∏•‡∏±‡∏ö‡πÑ‡∏õ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏ö‡∏≠‡∏Å‡∏ß‡πà‡∏≤‡πÇ‡∏î‡∏ô‡πÅ‡∏ö‡∏ô**
    return true
end

-- **‡∏ñ‡πâ‡∏≤‡πÑ‡∏°‡πà‡πÇ‡∏î‡∏ô‡πÅ‡∏ö‡∏ô ‡∏™‡πà‡∏á‡∏Ñ‡πà‡∏≤ false ‡∏Å‡∏•‡∏±‡∏ö‡πÑ‡∏õ**
return false
