-- ‡∏Ñ‡πà‡∏≤‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô
local radius = 3
local numShots = 10
local maxRadius = 1000
local maxNumShots = 100000

-- ‡∏ï‡∏±‡∏ß‡πÄ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏£‡∏∞‡∏¢‡∏∞
local RadiusSlider = Tab:CreateSlider({
    Name = "‡∏Ñ‡∏ß‡∏ö‡∏Ñ‡∏∏‡∏°‡∏£‡∏∞‡∏¢‡∏∞",
    Range = {1, maxRadius},
    Increment = 1,
    Suffix = "‡∏£‡∏∞‡∏¢‡∏∞",
    CurrentValue = radius,
    Flag = "RadiusSlider",
    Callback = function(Value)
        radius = Value
    end,
})

-- ‡∏ï‡∏±‡∏ß‡πÄ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏à‡∏≥‡∏ô‡∏ß‡∏ô‡∏Å‡∏≤‡∏£‡∏¢‡∏¥‡∏á
local ShotsSlider = Tab:CreateSlider({
    Name = "‡∏à‡∏≥‡∏ô‡∏ß‡∏ô‡∏Å‡∏≤‡∏£‡∏¢‡∏¥‡∏á",
    Range = {1, maxNumShots},
    Increment = 1,
    Suffix = "‡∏Ñ‡∏£‡∏±‡πâ‡∏á",
    CurrentValue = numShots,
    Flag = "ShotsSlider",
    Callback = function(Value)
        numShots = Value
    end,
})

local enabled = false

local function updateScriptPosition()
    local player = game.Players.LocalPlayer
    local character = player.Character
    local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

    local angleStep = math.pi * 2 / numShots
    for i = 0, numShots - 1 do
        local angle = angleStep * i
        local offset = Vector3.new(math.cos(angle) * radius, 0, math.sin(angle) * radius)
        local targetPosition = humanoidRootPart.Position + offset

        local args = {
            [1] = targetPosition,
            [2] = targetPosition + Vector3.new(0, 0, 0)
        }

        game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("ZombieRelated"):WaitForChild("AcidSpit"):FireServer(unpack(args))
    end
end

local function toggle()
    enabled = not enabled
    if enabled then
        Rayfield:Notify({
            Title = "‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå‡πÄ‡∏õ‡∏¥‡∏î‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô",
            Content = "‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏ó‡∏≥‡∏á‡∏≤‡∏ô‡∏≠‡∏¢‡∏π‡πà!",
            Duration = 3
        })
    else
        Rayfield:Notify({
            Title = "‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå‡∏õ‡∏¥‡∏î‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô",
            Content = "‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå‡∏ñ‡∏π‡∏Å‡∏õ‡∏¥‡∏î‡πÅ‡∏•‡πâ‡∏ß!",
            Duration = 3
        })
    end
end

-- ‡∏õ‡∏∏‡πà‡∏°‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå
Tab:CreateButton({
    Name = "‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå",
    Callback = toggle
})

-- ‡∏•‡∏π‡∏õ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡πÅ‡∏•‡∏∞‡∏Å‡∏≤‡∏£‡∏¢‡∏¥‡∏á
task.spawn(function()
    while true do
        if enabled then
            updateScriptPosition()
        end
        task.wait(2)
    end
end)
